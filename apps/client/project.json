{
  "name": "@zknet/client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "outputs": ["{projectRoot}/dist"]
    },
    "clean": {
      "options": {
        "commands": [
          "rm -vrf ./dist",
          "rm -vrf ./src-tauri/gen/schemas",
          "rm -vrf ./src-tauri/target"
        ]
      }
    },
    "dev": {
      "executor": "nx:run-script",
      "options": {
        "script": "tauri dev"
      }
    }
  }
}
